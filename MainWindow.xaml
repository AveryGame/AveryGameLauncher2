                                </Storyboard>
                            </BeginStoryboard>
                        </EventTrigger>
                    </Style.Triggers>
                </Style>
            </Button.Style>
        </Button>
        <Image HorizontalAlignment="Left" Height="20" Margin="11,80,0,0" VerticalAlignment="Top" Width="21" Source="/outline_settings_ethernet_white_24dp.png"/>
        <Image HorizontalAlignment="Left" Height="22" Margin="11,110,0,0" VerticalAlignment="Top" Width="20" Source="/outline_bug_report_white_24dp.png"/>
        <TextBlock Text="Made by 1 24 9 with help from AveryMadness and Crunnie." HorizontalAlignment="Left" Margin="6,394,0,0" VerticalAlignment="Top" Width="130" Height="42" FontSize="10" FontFamily="Segoe UI Variable Text Light" Foreground="#FF888888"/>
        <TextBlock Text="AveryMadness, Sizzy, and Crunnie." HorizontalAlignment="Left" Margin="5,405,0,0" VerticalAlignment="Top" Width="144" Height="42" FontSize="9" FontFamily="Segoe UI Variable Text Light" Foreground="#FF888888"/>
        <Label HorizontalAlignment="Left" Margin="221,391,0,0" VerticalAlignment="Top" Foreground="White" FontFamily="Segoe UI Variable Text Light"/>
