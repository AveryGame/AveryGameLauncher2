        <Button Name="newpath" Click="addPath" Content="Add Avery Game manually" HorizontalAlignment="Left" Height="29" Margin="340,367,0,0" VerticalAlignment="Top" Width="140" Opacity="1" FontFamily="Corbel Light" Background="#FF211F1F" BorderBrush="Gainsboro" Foreground="White">
            <Button.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="15"/>
