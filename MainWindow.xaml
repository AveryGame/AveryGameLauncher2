                    </Style.Triggers>
                </Style>
            </Button.Style>
        </Button>
        <Image Name="pfp" Margin="5,422,769,2"/>
        <Border CornerRadius="7" BorderThickness="2" BorderBrush="#FF121212" Margin="5,422,769,2"/>
        <Border CornerRadius="7" BorderThickness="4" BorderBrush="#FF121212" Margin="2,420,766,-1"/>
        <Border BorderThickness="4" BorderBrush="#FF121212" Margin="2,420,766,-1"/>
        <Button Content="r" Margin="0,1,2,0" VerticalAlignment="Top" Height="14" Click="Close" FontFamily="Marlett" Background="#FF24252B" BorderBrush="#FF24252B" Foreground="White" Padding="0,0,0,0" HorizontalAlignment="Right" Width="14">
            <Button.Resources>
                <Style TargetType="Border">
