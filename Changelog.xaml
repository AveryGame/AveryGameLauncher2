<Window x:Class="AgsLauncherV2.Changelog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AgsLauncherV2"
        mc:Ignorable="d"
        Title="Changelog" Height="450" Width="800">
    <Grid Background="#FF24252B">
        <Border BorderBrush="#FF191919" BorderThickness="1000" Margin="0,-32,0,0" RenderTransformOrigin="0.5,0.5" HorizontalAlignment="Left" Width="144">
            <Border.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="0.589"/>
                    <RotateTransform/>
                    <TranslateTransform Y="0.74"/>
                </TransformGroup>
            </Border.RenderTransform>
            <Label Content="Label" HorizontalAlignment="Center" Height="0" VerticalAlignment="Center" Width="0"/>
        </Border>
        <Border BorderBrush="#FF121212" BorderThickness="1000" Height="30" VerticalAlignment="Top" Margin="0,404,0,0">
        </Border>
        <Button Content="Home" HorizontalAlignment="Left" Margin="6,50,0,0" VerticalAlignment="Top" Width="132" BorderBrush="{x:Null}" Foreground="White" Background="#FF64677B" FontFamily="Segoe UI Variable Text Light">
        </Button>
    </Grid>
</Window>
