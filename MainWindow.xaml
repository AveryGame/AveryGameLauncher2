        <TextBlock Name="ChangelogBugsTitle" Text="Home" HorizontalAlignment="Left" Margin="400,27,0,0" VerticalAlignment="Top" Width="181" TextAlignment="Center" Foreground="White" FontFamily="Segoe UI Variable Text Light" Height="73" FontSize="36"/>
        <TextBlock Name="VerSTR" HorizontalAlignment="Left" Margin="400,75,0,0" TextWrapping="Wrap" Text="Version 4.4" VerticalAlignment="Top" Width="181" TextAlignment="Center" Foreground="White" FontFamily="Segoe UI Variable Text Light" FontSize="14" Opacity="0"/>
        <TextBlock HorizontalAlignment="Left" Margin="278,82,0,0" TextWrapping="Wrap" Text="Welcome to the AGS Launcher!" VerticalAlignment="Top" Height="48" Width="426" Foreground="White" FontFamily="Segoe UI Variable Text Light" FontSize="32"/>
        <TextBlock HorizontalAlignment="Left" Margin="278,130,0,0" TextWrapping="Wrap" Text="Here is where you can read the latest patch notes, look at current bugs, download Avery Game, and launch it." VerticalAlignment="Top" Height="101" Width="426" Foreground="White" TextAlignment="Center" FontFamily="Segoe UI Variable Text Light" FontSize="24"/>
