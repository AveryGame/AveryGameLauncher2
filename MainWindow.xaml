                            </BeginStoryboard>
                        </EventTrigger>
                    </Style.Triggers>
                </Style>
            </Button.Style>
        </Button>
        <Button Name="testbutton" Content="Launch Avery Game" HorizontalAlignment="Left" Height="29" Margin="485,367,0,0" VerticalAlignment="Top" Width="140" Opacity="1" FontFamily="Corbel Light" Background="#FF211F1F" BorderBrush="Gainsboro" Foreground="White">
            <Button.Resources>
