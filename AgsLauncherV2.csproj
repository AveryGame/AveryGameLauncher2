<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>netcoreapp3.1</TargetFramework>
		<UseWPF>true</UseWPF>
		<ApplicationIcon>resources\launcher.ico</ApplicationIcon>
		<Authors>Kianna</Authors>
		<Version>2.6.0</Version>
		<Product>AgsLauncherV2</Product>
		<Description>Launcher for Avery Game by AveryMadness</Description>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<Optimize>True</Optimize>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<Optimize>False</Optimize>
	</PropertyGroup>

	<ItemGroup>
		<None Remove="333-3334785_png-transparent-info-icon-white-clipart.png" />
		<None Remove="ags.png" />
		<None Remove="agsgrey-1x.png" />
		<None Remove="houseicon.png" />
		<None Remove="NewPrev.png" />
		<None Remove="newspaper-128.png" />
		<None Remove="newspaper-24.png" />
		<None Remove="newspaper-48.png" />
		<None Remove="newspaper-512.png" />
		<None Remove="newspaper-64.png" />
		<None Remove="NewsPreview.png" />
		<None Remove="NPRE.png" />
		<None Remove="nuts.png" />
		<None Remove="outline_bug_report_white_24dp.png" />
		<None Remove="outline_home_white_24dp.png" />
		<None Remove="outline_settings_ethernet_white_24dp.png" />
		<None Remove="outline_settings_white_24dp.png" />
		<None Remove="R.png" />
		<None Remove="st.png" />
		<None Remove="white-house-icon-31.png" />
	</ItemGroup>

	<ItemGroup>
		<Content Include="resources\launcher.ico" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="DiscordRichPresence" Version="1.0.175" />
		<PackageReference Include="DocumentFormat.OpenXml" Version="2.16.0" />
		<PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.2.0" />
		<PackageReference Include="Microsoft.Toolkit.Uwp.Notifications" Version="7.1.2" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
		<PackageReference Include="System.IO.Compression.ZipFile" Version="4.3.0" />
		<PackageReference Include="WPFLocalizeExtension" Version="3.9.4" />
	</ItemGroup>

	<ItemGroup>
		<Resource Include="ags.png" />
		<Resource Include="agsgrey-1x.png" />
		<Resource Include="NewPrev.png" />
		<Resource Include="newspaper-128.png" />
		<Resource Include="newspaper-24.png" />
		<Resource Include="newspaper-48.png" />
		<Resource Include="newspaper-512.png" />
		<Resource Include="newspaper-64.png" />
		<Resource Include="NewsPreview.png" />
		<Resource Include="NPRE.png" />
		<Resource Include="outline_bug_report_white_24dp.png" />
		<Resource Include="outline_home_white_24dp.png" />
		<Resource Include="outline_settings_ethernet_white_24dp.png" />
		<Resource Include="outline_settings_white_24dp.png" />
		<Resource Include="st.png" />
	</ItemGroup>

	<ItemGroup>
		<Compile Update="Properties\Settings.Designer.cs">
			<DesignTimeSharedInput>True</DesignTimeSharedInput>
			<AutoGen>True</AutoGen>
			<DependentUpon>Settings.settings</DependentUpon>
		</Compile>
	</ItemGroup>

	<ItemGroup>
		<None Update="Properties\Settings.settings">
			<Generator>SettingsSingleFileGenerator</Generator>
			<LastGenOutput>Settings.Designer.cs</LastGenOutput>
		</None>
	</ItemGroup>

</Project>
